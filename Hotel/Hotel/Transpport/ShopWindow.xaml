<Window x:Class="Hotel.Transpport.ShopWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Hotel.Transpport"
        mc:Ignorable="d"
        Title="ShopWindow" Height="300" Width="300">
    <Grid RenderTransformOrigin="0.491,0.734">
        <TextBox x:Name="GuestViewData" HorizontalAlignment="Left" Height="25" Margin="54,37,0,0" TextWrapping="Wrap" Text="Add new shop" VerticalAlignment="Top" Width="178" IsEnabled="False" TextAlignment="Center"/>
        <Button x:Name="DodajDane" Content="Dodaj noew zlecenie" HorizontalAlignment="Left" Height="40" Margin="80,171,0,0" VerticalAlignment="Top" Width="122" RenderTransformOrigin="0.516,1.474" Click="DodajDane_Click_1"/>
        <TextBox x:Name="DataView" HorizontalAlignment="Left" Height="31" Margin="20,103,0,0" TextWrapping="Wrap" Text="Date" VerticalAlignment="Top" Width="95" TextAlignment="Center" IsEnabled="False"/>
        <DatePicker x:Name="DateData" HorizontalAlignment="Left" Height="31" Margin="147,103,0,0" VerticalAlignment="Top" Width="120"/>
    </Grid>
</Window>
