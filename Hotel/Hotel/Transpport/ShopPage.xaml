      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Hotel.Transpport"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="ShopPage">

    <Grid>
        <TextBox x:Name="DataView" HorizontalAlignment="Left" Height="31" Margin="10,108,0,0" TextWrapping="Wrap" Text="Date" VerticalAlignment="Top" Width="95" TextAlignment="Center" IsEnabled="False"/>
        <DatePicker x:Name="DateData" HorizontalAlignment="Left" Height="31" Margin="141,108,0,0" VerticalAlignment="Top" Width="120"/>

        <TextBox x:Name="Shop_View" HorizontalAlignment="Left" Height="25" Margin="53,10,0,0" TextWrapping="Wrap" Text="Shop View" VerticalAlignment="Top" Width="179" IsEnabled="False" TextAlignment="Center"/>
        <Button x:Name="DodajDane" Content="Dodaj noew zlecenie" HorizontalAlignment="Left" Height="38" Margin="82,216,0,0" VerticalAlignment="Top" Width="122"/>

    </Grid>
</Page>
