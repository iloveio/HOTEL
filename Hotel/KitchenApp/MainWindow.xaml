<Window x:Class="Kitchen.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Kitchen"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button x:Name="button" Content="Menu" HorizontalAlignment="Left" Margin="203,94,0,0" VerticalAlignment="Top" Width="95" RenderTransformOrigin="-0.403,0.61" Click="button_Click"/>
        <Button x:Name="button1" Content="Ingredients" HorizontalAlignment="Left" Margin="203,137,0,0" VerticalAlignment="Top" Width="95" Click="button1_Click"/>
        <Button x:Name="button2" Content="Orders History" HorizontalAlignment="Left" Margin="203,183,0,0" VerticalAlignment="Top" Width="95" Click="button2_Click"/>
        <Button x:Name="button3" Content="Income" HorizontalAlignment="Left" Margin="203,224,0,0" VerticalAlignment="Top" Width="95" Click="button3_Click"/>
        <Label x:Name="label" Content="Kitchen Module" FontSize="20"  HorizontalAlignment="Left" Margin="176,30,0,0" VerticalAlignment="Top" Width="154"/>

    </Grid>
</Window>
