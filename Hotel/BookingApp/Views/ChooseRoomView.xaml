<UserControl x:Class="BookingApp.Views.ChooseRoomView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:BookingApp.Views"
             mc:Ignorable="d"
             DataContext="{Binding ChooseRoom, Mode=OneWay, Source={StaticResource Locator}}"
             d:DesignHeight="300" Width="492.982">
    <Grid>

        <Button x:Name="Room1Button" x:Uid="1" Content="Pokój 1" HorizontalAlignment="Left" Height="28"
                Margin="54,99,0,0" VerticalAlignment="Top" Width="77" Command="{Binding ChangeSelectedRoomCommand}" CommandParameter="{Binding RelativeSource={RelativeSource Self}, Path=Uid}" />
        <Button x:Name="Room2Button" x:Uid="45" Content="Pokój 45" HorizontalAlignment="Left" Height="28"
                Margin="170,99,0,0" VerticalAlignment="Top" Width="77" RenderTransformOrigin="1.909,0.643"
                Command="{Binding ChangeSelectedRoomCommand}" CommandParameter="{Binding RelativeSource={RelativeSource Self}, Path=Uid}" />
    </Grid>
</UserControl>