<Window x:Class="Transport.Transpport.ShopWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Transport.Transpport"
        mc:Ignorable="d"
        Title="ShopWindow" Height="300" Width="300">
    <Grid>
        <TextBox x:Name="Shop_View" HorizontalAlignment="Left" Height="25" Margin="53,10,0,0" TextWrapping="Wrap" Text="Shop View" VerticalAlignment="Top" Width="179" IsEnabled="False" TextAlignment="Center"/>
        <Button x:Name="DodajDane" Content="Dodaj noew zlecenie" HorizontalAlignment="Left" Height="38" Margin="75,204,0,0" VerticalAlignment="Top" Width="122" RenderTransformOrigin="0.494,1.218" Click="DodajDane_Click"/>
        <TextBox x:Name="DataViewZ" HorizontalAlignment="Left" Height="31" Margin="37,62,0,0" TextWrapping="Wrap" Text="Date" VerticalAlignment="Top" Width="95" TextAlignment="Center" IsEnabled="False" RenderTransformOrigin="0.57,-3.382"/>
        <DatePicker x:Name="DateData" HorizontalAlignment="Left" Height="31" Margin="157,60,0,0" VerticalAlignment="Top" Width="120" RenderTransformOrigin="0.5,0.5">
        </DatePicker>
    </Grid>
</Window>
