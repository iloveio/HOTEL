<Window x:Class="Transport.Transpport.MenagerPage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Transport.Transpport"
        mc:Ignorable="d"
        Title="MenagerPage" Height="300" Width="300">
    <Grid>
        <ListBox x:Name="listBox" HorizontalAlignment="Left" Height="148" Margin="26,40,0,0" VerticalAlignment="Top" Width="230" SelectionChanged="listBox_SelectionChanged"/>
        <TextBox x:Name="information" HorizontalAlignment="Left" Height="25" Margin="26,10,0,0" TextWrapping="Wrap" Text="Chose order" VerticalAlignment="Top" Width="184" IsEnabled="False"/>
        <Button x:Name="button" HorizontalAlignment="Left" Height="37" Margin="26,193,0,0" VerticalAlignment="Top" Width="75">
            <Button x:Name="Delete" Content="Delete" Width="74" Click="Edit_Click"/>
        </Button>
        <Button x:Name="Finish" Content="Finish" HorizontalAlignment="Left" Height="37" Margin="101,193,0,0" VerticalAlignment="Top" Width="87" Click="Finish_Click"/>
        <Button x:Name="Edit" Content="Edit" HorizontalAlignment="Left" Height="37" Margin="188,193,0,0" VerticalAlignment="Top" Width="68"/>

    </Grid>
</Window>
